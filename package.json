{
  "name": "artifyfun-vue-components",
  "author": "artifyfun <artifyfun@mail.com> (https://github.com/artifyfun)",
  "version": "0.1.1",
  "license": "MIT",
  "private": false,
  "description": "Artifyfun Vue Components For Low Code Development",
  "contributors": [
    {
      "name": "artifyfun",
      "email": "artifyfun@mail.com"
    }
  ],
  "keywords": [
    "some",
    "keywords",
    "here"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/artifyfun/artifyfun-vue-components.git"
  },
  "homepage": "https://artifyfun.github.io/artifyfun-vue-components/",
  "bugs": {
    "url": "https://github.com/artifyfun/artifyfun-vue-components/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm test && vite build --mode production && pnpm build:pages",
    "build:lib": "pnpm test && vite build --mode production",
    "deploy": "pnpm build:lib && node ./scripts/deploy.js",
    "dp": "node ./scripts/deploy.js",
    "clean": "rm -rf ./dist && rm -rf ./live-demo && rm -rf ./coverage && rm -rf ./storybook-static",
    "coverage:web": "http-server ./coverage",
    "changeset": "changeset",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run  --coverage",
    "build:pages": "pnpm docs:build && pnpm build:live-demo && pnpm build-storybook",
    "build:live-demo": "vite build --mode live-demo --outDir ./docs/.vitepress/dist/live-demo",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir ./docs/.vitepress/dist/storybook",
    "release": "npm publish --access public"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.7",
    "dayjs": "^1.11.11",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/motion": "^2.1.0",
    "vue": "3.4.23",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.5.3",
    "@storybook/vue3-vite": "^7.5.3",
    "@types/node": "^20.9.4",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^0.34.6",
    "defu": "^6.1.3",
    "less": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vitepress": "^1.0.1",
    "vitest": "^0.34.6"
  }
}
